<% include ../__header %>
<div class="<%= clase %>"><%= mensaje %></div>
<div class="row container">
    <div class="col-md-12">
        <h3>Agregar zombie</h3>
        <form action="nuevo" method="POST">
            <div class="form-group">
                <label for="">Nombre</label>
                <input type="text" class="form-control" name="name" autocomplete="off">
            </div>
            <div class="form-group">
                <label for="">Correo electronico</label>
                <input type="text" class="form-control" name ="email" autocomplete="off">
            </div>
            <div class="form-group">
                <label for="">Tipo</label>
                <input type="text" class="form-control" name="type" autocomplete="off">
            </div>
            <button class="btn btn-primary"><i class="fas fa-skull"></i> Agregar zombie</button>
        </form>
    </div>
</div>
<% include ../__footer %>